<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons"
              rel="stylesheet">
        {{ encore_entry_link_tags('app') }}

    {% endblock %}
</head>
<body>
{{ include("app/includes/header.html.twig") }}

<div class="container">
    {% block body %}{% endblock %}
</div>
{% block footer %}{% endblock %}

{% block javascripts %}

    {{ encore_entry_script_tags('app') }}

{% endblock %}
</body>
</html>
